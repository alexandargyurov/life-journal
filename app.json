{
  "expo": {
    "name": "Life Tracker",
    "slug": "life-tracker",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "0.2.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#C7EECA"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.alexandargyurov.lifetracker",
      "supportsTablet": false,
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-6414919472390529~2221508850",
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.410790811034-b38njme8u8cr1mq2ihnl7tg8soue68it"
        }
      }
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "js",
        "jsx",
        "css"
      ]
    },
    "description": "The all-in-one app to track your day-to-day life. Record your mood, take notes, link photos and even keep a voice a diary. View a heatmap of your feelings and notice your trends on what factors are effecting your mood.",
    "githubUrl": "https://github.com/alexandargyurov/lifetracker-app",
    "android": {
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-6414919472390529~5965312748"
      },
      "package": "com.alexandargyurov.lifetracker",
      "googleServicesFile": "./google-services.json",
      "permissions": [],
      "versionCode": 8
    }
  }
}

{
  "expo": {
    "scheme": "lifetracker",
    "name": "Life Tracker",
    "description": "The all-in-one app to track your day-to-day life. Record your mood, take notes, link photos and even keep a voice a diary.",
    "slug": "life-tracker",
    "privacy": "public",
    "githubUrl": "https://github.com/alexandargyurov/lifetracker-app",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#C7EECA"
    },
    "facebookAutoInitEnabled": false,
    "facebookAdvertiserIDCollectionEnabled": false,
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "buildNumber": "11",
      "bundleIdentifier": "com.alexandargyurov.lifetracker",
      "supportsTablet": false
    },
    "android": {
      "package": "com.alexandargyurov.lifetracker",
      "permissions": [],
      "versionCode": 11
    },
    "notification": {
      "icon": "./assets/icon.png"
    }
  }
}
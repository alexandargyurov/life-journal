{
  "expo": {
    "name": "Life Tracker",
    "slug": "life-tracker",
    "privacy": "public",
    "sdkVersion": "34.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#C7EECA"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "js",
        "jsx",
        "css"
      ]
    },
    "description": "Track your feelings each day",
    "githubUrl": "https://github.com/alexandargyurov/life-tracker-mobile",
    "android": {
      "package": "com.alexandargyurov.lifetracker",
      "permissions": [],
      "versionCode": 5
    }
  }
}
